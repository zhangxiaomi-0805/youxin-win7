{
  "name": "youxin",
  "version": "3.0.9",
  "author": "Netease",
  "description": "优信企业办公解决方案桌面端",
  "displayName": "",
  "homepage": "http://netease.im/nice",
  "keywords": [
    "electron",
    "优信",
    "企业办公"
  ],
  "os": [
    "darwin",
    "win32"
  ],
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "build:mac": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "package": "electron-packager ./ --platform=win32 --arch=ia32 --app-version=0.0.1 --electron-version=2.0.3 --icon=./build/icons/icon.ico --overwrite",
    "package:x86": "electron-packager ./ --platform=win32 --arch=ia32 --app-version=0.0.1 --electron-version=2.0.3 --icon=./build/icons/icon.ico --overwrite",
    "package:x64": "electron-packager ./ --platform=win32 --arch=x64 --app-version=0.0.1 --electron-version=2.0.3 --icon=./build/icons/icon.ico --overwrite",
    "package:mac": "electron-packager ./ --platform=darwin --arch=x64 --app-version=0.0.1 --electron-version=2.0.3 --icon=./build/icons/icon.icns --overwrite"
  },
  "build": {
    "productName": "YouXin",
    "appId": "org.netease.nice",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "title": "YouXin",
      "icon": "static/img/logo.icns",
      "iconSize": "102",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.business"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "crypto-js": "^3.1.9-1",
    "electron-cookies": "^1.1.0",
    "localStorage": "^1.0.3",
    "vue": "^2.3.3",
    "vue-contextmenu": "^1.4.8",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.3",
    "electron-packager": "^12.1.0",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
